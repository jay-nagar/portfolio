{
  "name": "dopefolio",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile:scss": "sass --watch sass/main.scss css/style.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.css -o css/style.css",
    "compress:css": "sass --style=compressed sass/main.scss css/style.css",
    "build": "npm-run-all prefix:css compress:css"
  },
  "author": "",
  "license": "GPL-3.0",
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "sass": "^1.66.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0"
  }
}